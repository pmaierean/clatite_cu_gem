{
  "name": "sample-client",
  "version": "2.2.0",
  "description": "A generic converter of JS from Typescript",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f ./app*.js; rm -f ./*.js.map",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "buildDev": "webpack --config webpack.dev.js --progress --profile --bail",
    "build": "webpack --config webpack.prod.js --progress --profile --bail",
    "go": "concurrent \"npm run tsc:w\" \"npm run serve\" "
  },
